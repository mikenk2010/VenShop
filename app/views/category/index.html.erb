<div class="container-home">
  <div class="items">
    <div class="clear" />
    <div class="newest">
      <div class="headline-newest">List all products - Cate</div>
     <% Product.all.each do |product| %>
      <div class="list-Browse-NewsRv">
        <div class="bl-image"><a href="/item">
          <img src="<%= product.img %>" />
        </a>
        </div>
        <div class="price"><%= product.price %> Y
          <div class="newest-datetime"><%= product.date %>


          </div>
        </div>
        <div class="title-Br-NewsRv"><a href="/item"><%= product.title %></a>
        </div>
        <div class="Br-NewsRv-cont">
          <%= product.desc %>
        </div>
      </div>
      <% end %>
    </div>
  </div>
</div>